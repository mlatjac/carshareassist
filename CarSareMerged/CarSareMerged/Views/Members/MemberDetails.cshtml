@model CarSareMerged.Models.Member
@{
    ViewBag.Title = "MemberDetails";
}

<h2>MemberDetails</h2>
<table class="table">
    <tr><th>ID</th><td>@Model.ID</td></tr>
    <tr><th>Name</th><td>@Model.FName @Model.LName</td></tr>
    <tr><th>Email</th><td>@Model.Email</td></tr>
    <tr><th>Phone</th><td>@Model.Phone</td></tr>
    
</table>
<button class="btn">@Html.ActionLink("Delete", "Delete", "Members", new { id = @Model.ID }, null)</button>
<button class="btn">@Html.ActionLink("Edit", "Edit", "Members", new { id = @Model.ID }, null)</button>
